@{
    var priceRecord = ViewData["Prices"] as List<PriceRecord>;
}
@{
    <div class="text-center">
        <h1 class="display-4">Crypto Prices</h1>
    </div>

    <table>
        <tr>
            <th>BuyPrice</th>
            <th>SellPrice</th>
            <th>Type</th>
        </tr>
        <tr>
        </tr>
    </table>
    @foreach (var item in @priceRecord)
    {
        <li>@item.BuyPrice</li>
        <li>@item.SellPrice</li>
        <li>@item.Client</li>

    }
}
